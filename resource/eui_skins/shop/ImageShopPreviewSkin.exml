            <e:layout>
                <e:HorizontalLayout verticalAlign="middle" gap="180" />
            </e:layout>
        </e:Group>
    </e:Group>
    <e:Button id="closeBtn" top="50" right="70" width="91" height="91">
        <e:skinName>
            <e:Skin states="up,down,disabled">
                <e:Image scaleX.down="0.9" scaleY.down="0.9" source="common_close_png" source.down="common_close_png"
                         horizontalCenter="0" verticalCenter="0" width="91" height="91" />
            </e:Skin>
        </e:skinName>
    </e:Button>
    <e:Image id="datu" touchEnabled="true" horizontalCenter="0" verticalCenter="0"
              source=""/>

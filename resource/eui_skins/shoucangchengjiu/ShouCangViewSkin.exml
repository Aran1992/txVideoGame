		<e:Button id="bgBtn" label="已获福利" width="200" x="40" y="28" height="62">
			<e:skinName>
				<e:Skin states="up,down,disabled" width="190" xmlns:n1s="*" height="62">
					<e:Group width="100%" height="100%" />
					<e:Image scaleX.down="0.9" scaleY.down="0.9" source="common_close1_png" source.down="common_close1_png"

			<e:Group id="itemGroup" touchChildren="true" touchEnabled="true" width="100%" height="100%">
				<e:Scroller id="scroll" height="100%" skinName="skins.ScrollerSkin" horizontalCenter="0" width="100%">
					<e:Group id="goodsLayer">
						<e:layout>
							<e:TileLayout requestedColumnCount="4" horizontalGap="50" verticalGap="30"/>
